@model List<Blob>

<div class="p-4 container">
	<div class="row">
		<h2 class="text-primary">Images</h2>
	</div>
	<div class="row">
		@foreach(var item in Model)
		{
			<div class="col-4 pt-4 border">
				<h4 class="text-success">@item.Title</h4>
				<img src="@item.Uri" alt="@item.Title" width="100%" />
			</div>
		}
	</div>
</div>